<%= form_for @editor, html: {multipart: true} do |f| %>
  <% if @editor.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@editor.errors.count, "error") %> prohibited this @editor from being saved:</h2>

      <ul>
      <% @editor.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>

  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>

  <div class="field">
    <%= f.label :company_name %>
    <%= f.text_field :company_name %>
  </div>

  <div class="field">
    <%= f.label :pleft %>
    <%= f.text_area :pleft %>
  </div>

  <div class="field">
    <%= f.label :pcenter %>
    <%= f.text_area :pcenter %>
  </div>

  <div class="field">
    <%= f.label :pright %>
    <%= f.text_area :pright %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
